                        <input name="correo" type="text"  placeholder="ejemplo@gmail.com" autofocus="" required=""></p>	
					
					<p><label >Tu mensaje:</label><br />
		            <textarea name="consulta" class="input"  rows="7" cols="30"></textarea><br />
 
                    <p id="bot"><input type="submit" name="submit" value="Registrar" class="boton" autofocus="" required=""></p>
                </form>
